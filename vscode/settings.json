{
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "scm.diffDecorations": "none",
  "breadcrumbs.enabled": false,
  "git.decorations.enabled": false,
  "workbench.editor.showTabs": "none",
  "window.menuBarVisibility": "compact",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.lineNumbers": "relative",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "solid",
  "editor.fontWeight": 500,
  "editor.formatOnSave": false,
  "workbench.startupEditor": "none",
  "vim.useSystemClipboard": true,
  "vim.incsearch": true,
  "vim.hlsearch": false,
  "vim.sneak": false,
  "vim.surround": true,
  "vim.easymotion": false,
  "vim.leader": "<space>",
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "s"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "before": ["g", "r", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["g", "r", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "r", "y"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "r", "h"],
      "commands": ["editor.action.triggerParameterHints"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "n"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": [":bd"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "Q"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["ctrl+p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "c", "s"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
      "before": ["<leader>", "c", "S"],
      "commands": ["workbench.action.terminal.new"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.changeAll"]
    },
    {
      "before": ["<leader>", "l", "r"],
      "commands": ["editor.action.refactor"]
    },
    {
      "before": ["g", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["g", "r", "a"],
      "commands": ["editor.action.codeAction"]
    },
    {
      "before": ["<leader>", "l", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "l", "o"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["<leader>", "l", "s"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "l", "w", "s"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": ["<leader>", "b", "l"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "d", "l"],
      "commands": ["workbench.actions.view.problems"]
    },
    {
      "before": ["<leader>", "l", "g"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<leader>", "<leader>", "k"],
      "commands": ["workbench.action.closeOtherEditors"]
    },
    {
      "before": ["<leader>", "<leader>", "K"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    {
      "before": ["<leader>", "<leader>", "l"],
      "commands": ["projectManager.listProjects"]
    },
    {
      "before": ["<leader>", "<leader>", "S"],
      "commands": ["projectManager.saveProject"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["workbench.action.findInFiles", "extension.vim_escape"]
    },
    {
      "before": ["<leader>", "*"],
      "commands": [
        "editor.action.addSelectionToNextFindMatch",
        "workbench.action.findInFiles",
        "extension.vim_escape"
      ]
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "e", "x"],
      "commands": ["workbench.view.extensions"]
    },
    {
      "before": ["<leader>", "e", "c"],
      "commands": ["workbench.panel.chatSidebar.copilot"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["extension.vim_ctrl+^"]
    },
    {
      "before": ["-"],
      "commands": ["workbench.action.files.openFolder"]
    },
    {
      "before": ["<leader>", "u", "m"],
      "commands": ["workbench.action.openSettingsJson"]
    },
    {
      "before": ["<leader>", "u", "k"],
      "commands": ["workbench.action.openGlobalKeybindingsFile"]
    },
    {
      "before": ["<leader>", "u", "t"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "c", "m"],
      "after": ["*", "N", "c", "g", "n"]
    },
    {
      "before": ["<leader>", "c", "M"],
      "after": ["*", "N", "c", "g", "N"]
    },
    {
      "before": ["ctrl+n"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "*"],
      "commands": ["workbench.action.findInFiles", "extension.vim_escape"]
    },
    {
      "before": ["ctrl+n"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    },
    {
      "before": ["ctrl+p"],
      "commands": ["editor.action.addSelectionToPreviousFindMatch"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.changeAll"]
    },
    {
      "before": ["<leader>", "c", "m"],
      "commands": ["editor.action.selectHighlights"]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["ctrl+h"],
      "commands": ["editor.action.triggerParameterHints"]
    }
  ],
  "java.codeGeneration.toString.codeStyle": "STRING_BUILDER",
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.format.settings.profile": "GoogleStyle",
  "java.codeGeneration.generateComments": true,
  "java.codeGeneration.hashCodeEquals.useJava7Objects": true,
  "java.codeGeneration.hashCodeEquals.useInstanceof": true,
  "javascript.format.semicolons": "insert",
  "typescript.format.semicolons": "insert",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "errorLens.enabledDiagnosticLevels": ["warning", "info", "error"]
}
